    <add key="PATH_IMMAGINI_COLLABORATORI" value="~/Images/COLLABORATORI/" />
    <add key="PATH_DOCUMENTI_COLLABORATORI" value="~/Images/DOCUMENTI/" />
    <add key="PATH_TEMPLATE" value="~/TEMPLATE/" />
    <add key="IMMAGINE_DUMMY_COLLABORATORE" value="dummy.jpg" />
    <add key="QUERY_SEARCH_COLLABORATORI" value="select top 200 coll.id,Cognome,Nome,codicefiscale as CF,Nazione,comunenascita as [Comune Nascita],convert(varchar,datanascita,103) as [Data Nascita],comuneriferimento as [Comune Rif],regioneRiferimento as [Regione Rif], nomeSocieta as Societa, partitaiva as [P.Iva],convert(bit,assunto) as Assunto from anag_collaboratori coll left join anag_qualifiche_collaboratori qual on coll.id = qual.id_collaboratore where coll.attivo = 1 and (cognome like '%@cognome%' or cognome is null) and (nome like '%@nome%' or nome is null) and (codiceFiscale like '%@codiceFiscale%' or codiceFiscale is null) and (comuneRiferimento like '%@comuneRiferimento%' or comuneRiferimento is null) and (regioneRiferimento like '%@regioneRiferimento%' or regioneRiferimento is null) and (partitaIva like '%@partitaIva%' or partitaIva is null) and (nomeSocieta like '%@nomeSocieta%' or nomeSocieta is null) and isnull(qualifica,'') like '%@qualifica%' group by coll.id,Cognome,Nome,codicefiscale,Nazione,comunenascita,convert(varchar,datanascita,103),comuneriferimento,regioneRiferimento, nomesocieta, partitaiva,convert(bit,assunto) order by Cognome,Nome" />
    <add key="QUERY_SEARCH_AZIENDE" value="select top 200   azienda.id,ragioneSociale,partitaIva,codicefiscale,codiceIdentificativo,tipo  from   anag_clienti_fornitori azienda   left join anag_referente_clienti_fornitori ref  on   azienda.id = ref.id_azienda  where   azienda.attivo = 1  and @ClienteFornitore   and (ragioneSociale like '%@ragioneSociale%' or ragioneSociale is null)   and (partitaIva like '%@partitaIva%' or partitaIva is null)   and (codiceFiscale like '%@codiceFiscale%' or codiceFiscale is null)   and (codiceIdentificativo like '%@codiceIdentificativo%' or codiceIdentificativo is null)   and (tipo like '%@tipo%' or tipo is null)   and isnull(cognome,'') like '%@cognome%'  group by   azienda.id,ragioneSociale,partitaIva,codicefiscale,codiceIdentificativo,tipo  order by   ragioneSociale" />
    <add key="QUERY_SEARCH_ARTICOLI" value="select id, defaultDescrizioneLunga as Descrizione, defaultDescrizione as [Desc. breve],defaultPrezzo as prezzo ,defaultCosto as costo,defaultIva as IVA 
from art_articoli art
where art.attivo = 1
AND (defaultDescrizioneLunga like '%@defaultDescrizioneLunga%' or defaultDescrizioneLunga is null)
AND (defaultDescrizione like '%@defaultDescrizione%' or defaultDescrizione is null)
AND (defaultPrezzo like '%@defaultPrezzo%' or defaultPrezzo is null)
AND (defaultCosto like '%@defaultCosto%' or defaultCosto is null)
AND (defaultIva like '%@defaultIva%' or defaultIva is null)
ORDER BY defaultDescrizioneLunga" />
    <add key="QUERY_SEARCH_PROTOCOLLI" value="select top 200   prot.id,prot.codice_lavoro,prot.numero_protocollo,prot.cliente,tipo.nome as Tipo,prot.protocollo_riferimento as Riferimento  
from   dati_protocollo prot   
left join tipo_protocollo tipo  
on   prot.id_tipo_protocollo = tipo.id  
where   prot.attivo = 1  
and (prot.numero_protocollo like '%@numeroProtocollo%' or numero_protocollo is null)   
and (prot.codice_lavoro like '%@codiceLavoro%' or codice_lavoro is null)   
and (prot.cliente like '%@cliente%' or prot.cliente is null)   
and (prot.protocollo_riferimento like '%@protocolloRiferimento%' or prot.cliente is null)
and isnull(tipo.nome,'') like '%@tipoProtocollo%'  
group by   prot.id,prot.codice_lavoro,prot.numero_protocollo,prot.cliente,tipo.nome,prot.protocollo_riferimento  
order by   prot.codice_lavoro,prot.numero_protocollo"/>
    <add key="NUMERO_PROTOCOLLO" value="VS-@anno-@protocollo"/>
    <add key="FORMAT_NUMERO_PROTOCOLLO" value="0000000"/>
    <add key="CODICE_LAVORAZIONE" value="@anno@codiceLavorazione"/>
    <add key="FORMAT_CODICE_LAVORAZIONE" value="00000"/>

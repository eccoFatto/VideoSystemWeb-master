    <add key="QUERY_SEARCH_COLLABORATORI" value="select top 200 coll.id,Cognome,Nome,codicefiscale as CF,Nazione,comunenascita as [Comune Nascita],provinciaNascita as Prov,convert(varchar,datanascita,103) as [Data Nascita],comuneriferimento as [Comune Rif], nomeSocieta as Societa, partitaiva as [P.Iva],convert(bit,assunto) as Assunto from anag_collaboratori coll left join anag_qualifiche_collaboratori qual on coll.id = qual.id_collaboratore where coll.attivo = 1 and (cognome like '%@cognome%' or cognome is null) and (nome like '%@nome%' or nome is null) and (codiceFiscale like '%@codiceFiscale%' or codiceFiscale is null) and (comuneRiferimento like '%@comuneRiferimento%' or comuneRiferimento is null) and (partitaIva like '%@partitaIva%' or partitaIva is null) and (nomeSocieta like '%@nomeSocieta%' or nomeSocieta is null) and isnull(qualifica,'') like '%@qualifica%' group by coll.id,Cognome,Nome,codicefiscale,Nazione,comunenascita,provinciaNascita,convert(varchar,datanascita,103),comuneriferimento, nomesocieta, partitaiva,convert(bit,assunto) order by Cognome,Nome" />
    <add key="QUERY_SEARCH_AZIENDE" value="select top 200   azienda.id,ragioneSociale,partitaIva,codicefiscale,codiceIdentificativo,tipo  from   anag_clienti_fornitori azienda   left join anag_referente_clienti_fornitori ref  on   azienda.id = ref.id_azienda  where   azienda.attivo = 1  and cliente = @cliente  and fornitore = @fornitore   and (ragioneSociale like '%@ragioneSociale%' or ragioneSociale is null)   and (partitaIva like '%@partitaIva%' or partitaIva is null)   and (codiceFiscale like '%@codiceFiscale%' or codiceFiscale is null)   and (codiceIdentificativo like '%@codiceIdentificativo%' or codiceIdentificativo is null)   and (tipo like '%@tipo%' or tipo is null)   and isnull(cognome,'') like '%@cognome%'  group by   azienda.id,ragioneSociale,partitaIva,codicefiscale,codiceIdentificativo,tipo  order by   ragioneSociale" />
  </appSettings>
